  tls andrew.the.techie@gmail.com

  root /var/www/html/wp-content
  gzip

  fastcgi / wordpress:9000 php
  rewrite {
    if {path} not_match ^\/wp-admin
    to {path} {path}/ /index.php?_url={uri}
  }
  log stdout
  errors stderr
 }


